{
  "name": "pawcapella-www",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/pawcapella/www",
  "author": "Nick Iaconis <nick.iaconis+github@gmail.com>",
  "license": "none",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js --progress --colors",
    "build": "webpack --config ./webpack.config.prod.js --progress --colors",
    "postbuild": "react-snap",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@css-blocks/jsx": "^0.20.0",
    "@css-blocks/runtime": "^0.20.0",
    "@css-blocks/webpack": "nickiaconis/css-blocks#css-blocks-webpack-v0.20.0-gitpkg",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.11",
    "@types/webpack-env": "^1.13.9",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.3.1",
    "prettier": "1.16.4",
    "pretty-quick": "^1.10.0",
    "react-hot-loader": "^4.8.0",
    "react-snap": "^1.23.0",
    "react-test-renderer": "^16.8.4",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.3",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "obj-str": "^1.0.3",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead",
    "not ie > 0"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.block.css$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/test-setup.ts"
    ],
    "testRegex": "(/tests/.*|\\.?test)\\.tsx?$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "prettier": {
    "endOfLine": "lf",
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "reactSnap": {
    "source": "dist"
  }
}
